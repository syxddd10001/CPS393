echo
<, >, >>, 2>, 2>>  #input/output redirection
/dev/null          #null device (garbage can)
/dev/stdin, /dev/stdout, /dev/stderr
&0, &1, &2         #stdin, stdout, stderr
ctrl-d, ctrl-c     #end stdin, kill program
#comment
?, *, [ ]          #glob constructs 
[:alnum:], [:lower:], [:upper:] etc #character classes
~                  #user's home dir
~xyz               #/home/xyz
~+                 #current working directory
~-                 #previous working directory
-                  #previous working directory
" ", ' '           #protect characters inside
\                  #protect next character
shopt -s extglob   #set extended globs
 *(exp)            #0 or more occurrences of exp
 +(exp)            #1 or more occurrences of exp
 ?(exp)            #0 or 1 occurrences of exp
 @(exp1|exp2|...)  #exp1 or exp2 or ...
 !(exp)            #anything that doesn't match exp
#!/bin/bash        #first line of a bash shell program
$1, $2, ...        #arguments to shell program
$0                 #name of shell program
$#                 #number of arguments to shell program
$@, $*             #all arguments to shell program
grep
egrep
find
head, tail
cut, paste


